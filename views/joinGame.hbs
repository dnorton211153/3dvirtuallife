<table class="table table-striped table-inverse table-responsive">
    <thead class="thead-inverse">
        <tr>
            <th>Namespace</th>
            <th>Level</th>
            <th>Description</th>
        </tr>
        </thead>
        <tbody>
            {{#each activeGames}}
            <tr>
                <td scope="row">{{@key}}</td>
                <td>{{this.level}}</td>
                <td></td>
                <td><button type="button" id="{{@key}}" class="btn btn-primary btn-block btn-sm join"><i class="fas fa-box-open noClick"></i></button></td>
            </tr>
            {{/each}}
        </tbody>
</table>
