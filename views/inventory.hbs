<table class="table table-striped table-inverse table-responsive">
    <thead class="thead-inverse">
        <tr>
            <th>Item</th>
            <th>Description</th>
            <th>Quantity</th>
        </tr>
        </thead>
        <tbody>
            {{#each this}}
            <tr>
                {{!-- <td scope="row">{{@key}}</td>
                <td></td>
                <td>{{this}}</td> --}}
                <td>{{name}}</td>
                <td>{{description}}</td>
                <td>{{quantity}}</td>
            </tr>
            {{/each}}
        </tbody>
</table>

